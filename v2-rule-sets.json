{
  "staticComplexRules": [
    {
      "description": "Mercutio Layer One",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_option"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ],
  "complexModificationRules": {
    "Mercutio Numbers": {
      "layer": "one",
      "manipulators": {
        "q": "1",
        "w": "2",
        "e": "3",
        "r": "4",
        "t": "5",
        "y": "6",
        "u": "7",
        "i": "8",
        "o": "9",
        "p": "0"
      }
    },
    "Mercutio VIM navigation": {
      "layer": "two",
      "manipulators": {
        "h": "left_arrow",
        "j": "down_arrow",
        "k": "up_arrow",
        "l": "right_arrow"
      }
    },
    "Mercutio F-Keys": {
      "layer": "two",
      "manipulators": {
        "q": "f1",
        "w": "f2",
        "e": "f3",
        "r": "f4",
        "t": "f5",
        "y": "f6",
        "u": "f7",
        "i": "f8",
        "o": "f9",
        "p": "f10",
        "a": "f11",
        "s": "f12"
      }
    },
    "Mercutio Punctuation": {
      "layer": "one",
      "manipulators": {
        "s": "grave_accent_and_tilde",
        "d": "backslash",
        "h": "quote",
        "j": "open_bracket",
        "k": "close_bracket",
        "l": "semicolon",
        "enter_or_return": "quote",
        "m": "hyphen",
        "comma": "equal_sign",
        "period": "slash",
        "delete_or_backspace": "delete_forward"
      }
    }
  }
}